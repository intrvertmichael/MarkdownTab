{
  "manifest_version": 2,
  "name": "Markdown Tab",
  "description": "A Markdown document when you open a new tab. Syncs with all your devices.",
  "version": "2.0.6",
  "content_security_policy": "script-src 'self' 'sha256-ZDQrfK5fVUbbjAYxJi/3oq72faM0Yo1up20yXlGMqBM='; object-src 'self'",
  "permissions": [ "storage" ],
  "chrome_url_overrides" : {
